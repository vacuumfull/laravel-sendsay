{
  "name": "sendsay-mailer",
  "require": {
    "guzzlehttp/guzzle": "^6.2",
    "php": ">=5.5.9"
  },
  "require-dev": {
    "mockery/mockery": "0.9.*",
    "phpunit/phpunit": "~6.1",
    "illuminate/config": "^5.4",
    "laravel/laravel": "^5.5"
  },
  "autoload": {
    "classmap": [
      "src/"
    ],
    "psr-4": {
      "SendsayMailer\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "SendsayMailer\\Providers\\SendsayMailerServiceProvider"
      ],
      "aliases": {
        "SendsayMailer": "SendsayMailer\\Facades\\SendsayMailerFacade"
      }
    }
  },
  "authors": [
    {
      "name": "Ildar Khairullin",
      "email": "mr.willentretenmert@gmail.com"
    }
  ]
}